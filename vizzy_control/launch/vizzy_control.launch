<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find vizzy_control)/config/vizzy_control.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="/vizzy" args="waist_position_controller neck_tilt_position_controller neck_pan_position_controller eyes_tilt_position_controller r_eye_pan_position_controller l_eye_pan_position_controller joint_state_controller"/>

